	<template name="DISPATCH_TABBAR">
		<container name="tabbarContainer" width="280" height="20" marginTop="41" marginLeft="15"
				   pixelSnapping="true">
			<itemlist name="tabsItemlist" registeredSkinName="commonui_list_mask" layout="horizontal" alignX="right"
					  width="280" itemGap="2" height="20" mouseEnabled="true" mouseChildren="true"
					  cacheSkinMaskAsBitmap="false">
				<itemrenderer width="139"  height="20" autoSizeWidth="false" mouseEnabled="true" mouseChildren="true">
					<button name="tab" registeredSkinName="dispatch_main_tab_btn" mouseEnabled="true"
							autoSizeWidth="true" autoSizeHeight="true" mouseChildren="true"/>
					<button name="tab_selected" registeredSkinName="dispatch_main_tab_btn.selected" mouseEnabled="true"
							autoSizeWidth="true" autoSizeHeight="true" mouseChildren="true" visible="false"/>
					<label y="-1" name="tabLabel" registeredSkinName="commonui_default_label"
						   registeredTextStyleName="dispatch_tab_center" mouseEnabled="false" marginTop="2"
						   mouseChildren="false" autoSizeWidth="true" text="?"/>
				</itemrenderer>
			</itemlist>
		</container>
	</template>


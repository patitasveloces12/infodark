    <!-- TEXT STYLE HERE -->
    <textstyle name         = "battlepass_confirmationbutton_blue_normal"
                textColor   = "0xF1FBEC"
                fontSize    = "16"
                type        = "dynamic"
                fontName    = "EurostileHeaFl"
                embedFonts  = "true"
                autoSize    = "center"
    />

    <textstyle name         = "battlepass_info"
               textColor    ="0x999898"
               type         ="dynamic"
               fontSize     ="9"
               autoSize     ="left"
               wordWrap     ="true"
               multiline    ="true"
               embedFonts   ="false"
               fontName     ="Verdana"
               align        ="left"
    />
    <textstyle name         = "battlepass_objectives_title"
            textColor       ="0xfefefe"
            type            ="dynamic"
            fontSize        ="10"
            autoSize        ="right"
            wordWrap        ="true"
            multiline       ="true"
            embedFonts      ="false"
            fontName        ="Verdana"
            bold            ="true"
            align           ="left"
    />
    <textstyle name         = "battlepass_objectives_condition"
            textColor       ="0xfefefe"
            type            ="dynamic"
            fontSize        ="10"
            autoSize        ="right"
            wordWrap        ="true"
            multiline       ="true"
            embedFonts      ="false"
            fontName        ="Verdana"
            align           ="left"
    />

    <textstyle name         = "battlepass_exchange_reward_description"
               textColor       ="0xfefefe"
               type            ="dynamic"
               fontSize        ="9"
               autoSize        ="right"
               wordWrap        ="true"
               multiline       ="true"
               embedFonts      ="false"
               fontName        ="Verdana"
               align           ="left"
    />

    <textstyle name         = "battlepass_objectives_progress"
            textColor       ="0xfefefe"
            type            ="dynamic"
            fontSize        ="10"
            autoSize        ="right"
            wordWrap        ="true"
            multiline       ="true"
            embedFonts      ="false"
            fontName        ="Verdana"
            align           ="right"
    />

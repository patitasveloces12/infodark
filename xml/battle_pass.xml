            textColor       ="0xffcc00"
            type            ="dynamic"
            fontSize        ="12"
            autoSize        ="left"
            wordWrap        ="false"
            multiline       ="false"
            embedFonts      ="false"
            fontName        ="Verdana"
            bold            ="false"
            align           ="right"
    />
    <textstyle name         = "battlepass_overview_ttip_title"
            textColor       ="0xfefefe"
            type            ="dynamic"
            fontSize        ="10"
            autoSize        ="right"
            wordWrap        ="true"
            multiline       ="true"
            embedFonts      ="false"
            fontName        ="Verdana"
            align           ="left"
            bold            ="true"
    />
    <textstyle name         = "battlepass_overview_ttip_content"
            textColor       ="0xfefefe"
            type            ="dynamic"
            fontSize        ="10"
            autoSize        ="right"
            wordWrap        ="true"
            multiline       ="true"
            embedFonts      ="false"
            fontName        ="Verdana"
            align           ="left"
            bold            ="false"
    />
    <textstyle name         = "battlepass_description_title"
            textColor       ="0xffcc00"
            type            ="dynamic"
            fontSize        ="12"
            autoSize        ="left"
            wordWrap        ="true"
            multiline       ="true"
            embedFonts      ="false"
            fontName        ="Verdana"
            bold            ="true"
            align           ="left"
    />
    <textstyle name         = "battlepass_description"
            textColor       ="0xededed"
            type            ="dynamic"
            fontSize        ="10"
            autoSize        ="left"
            wordWrap        ="true"
            multiline       ="true"
            embedFonts      ="false"
            fontName        ="Verdana"
            bold            ="false"
            align           ="left"
    />


    <!-- SKIN HERE -->
    <skin name="battlepass_invisible" classname="BasicSkin">
        <layer classname="FilledBoxLayer" lineColor="0x00ccff" fillAlpha="0">mask</layer>
    </skin>
    <skin name="battlepass_scrollbar_bgnd" classname="FadableButtonSkin">
        <layer classname="FilledBoxLayer" fillColor="0x0" fillAlpha="1">normal</layer>
    </skin>
    <skin name="battlepass_item_outline" classname="BasicSkin">
        <layer classname="FilledBoxLayer" lineColor="0x00ccff" fillAlpha="0" lineAlpha="1">normal</layer>
    </skin>
    <skin name="battlepass_ttip_frame" library="traininggroundsUI" classname="BasicSkin">
        <layer classname="BitmapLayer" scale9grid="1,1,2,2" libraryID="ttip_frame">normal</layer>
    </skin>
    <skin name="battlepass_reward_icon" library="battlePass" classname="BasicSkin">
        <layer classname="BitmapLayer" libraryID="rewards_icon">normal</layer>
    </skin>
    <skin name="battlepass_gold_reward_icon" library="battlePass" classname="BasicSkin">
        <layer classname="BitmapLayer" libraryID="gold_rewards_icon">normal</layer>
    </skin>
    <skin name="battlepass_gold_reward_blocker" library="battlePass" classname="BasicSkin">
        <layer classname="BitmapLayer" libraryID="item_gradient">normal</layer>
    </skin>
    <skin name="battlepass_gold_reward_lock" library="battlePass" classname="BasicSkin">
        <layer classname="BitmapLayer" libraryID="lock">normal</layer>
    </skin>
    <skin name="battlepass_obj_completed_blocker" classname="BasicSkin">
        <layer classname="FilledBoxLayer" fillColor="0x0" fillAlpha="0.7">normal</layer>
    </skin>
    <skin name="battlebass_outline" classname="BasicSkin">
        <layer classname="FilledBoxLayer" fillColor="0x0" fillAlpha="0" lineColor="0x37687F" lineAlpha="1">normal</layer>
    </skin>
    <skin name="battlepass_video_bg" classname="BasicSkin" library="battlePass">
        <layer classname="BitmapLayer" libraryID="item_bg">normal</layer>
    </skin>
    <skin name="battlepass_description_bg" classname="BasicSkin" library="battlePass">
        <layer classname="BitmapLayer" libraryID="frame_bp-description">normal</layer>
    </skin>
    <skin name="commonui_yellowlayer" classname="BasicSkin">
        <layer classname="FilledBoxLayer"  fillColor="0x999900" fillAlpha="0.3"  >normal</layer>
    </skin>
    <skin name="exchange_reward_special_bg" classname="BasicSkin" library="battlePass">
        <layer classname="BitmapLayer" libraryID="item_bg_yellow">normal</layer>
    </skin>
    <skin name="exchange_reward_standard_bg" classname="BasicSkin" library="battlePass">
        <layer classname="BitmapLayer" libraryID="item_bg">normal</layer>
    </skin>
    <skin name="exchange_description_special_bg" classname="BasicSkin" library="battlePass">
        <layer classname="BitmapLayer" libraryID="frame_bp-exchange_yellow">normal</layer>
    </skin>
    <skin name="exchange_description_standard_bg" classname="BasicSkin" library="battlePass">
        <layer classname="BitmapLayer" libraryID="frame_bp-exchange_blue">normal</layer>
    </skin>
    <skin name="exchange_key_token" classname="BasicSkin" library="battlePass">
        <layer classname="BitmapLayer" libraryID="battlepass-key">normal</layer>
    </skin>

    <!-- FILTER COMBO BOX HERE -->
    <textstyle name="assembly_label_filter"
               background="false"
               textColor="0xBFBFBF"
               type="dynamic"
               selectable="false"
               fontName="Verdana"
               embedFonts="false"
               fontSize="10"/>
    <textstyle name="equipment_combobox_inventory_filter"
               type="dynamic"
               selectable="false"
               fontName="Verdana"
               embedFonts="false"
               autoSize="left"
               align="left"
               textColor="0xC2BDC3"
               fontSize="10"/>
    <textstyle name="equipment_checkbox_inventory_filter"
               type="dynamic"
               align="left"
               autoSize="left"
               wordWrap="false"
               selectable="false"
               fontName="Verdana"
               embedFonts="false"
               textColor="0x6A7275"
               fontSize="10"
    />
    <skin name="crafting_combobox_filter" library="module_equipment_lib" classname="FadableButtonSkin">
        <layer classname="BitmapLayer" scale9grid="4,3,17,16" libraryID="equipment_combobox_filter_normal">normal
        </layer>
        <layer classname="BitmapLayer" scale9grid="4,3,17,16" libraryID="equipment_combobox_filter_hover">over</layer>
        <layer classname="BitmapLayer" scale9grid="4,3,17,16" libraryID="equipment_combobox_filter_active">clicked
        </layer>
    </skin>
    <skin name="crafting_background_inventory_filter_popup" library="module_equipment_lib" classname="BasicSkin">
        <layer classname="BitmapLayer" libraryID="equipment_background_filter_inventory2" scale9grid="11,18,15,25">
            normal
        </layer>
    </skin>
    <skin name="equipment_button_close_inventory_filter_popup" library="module_equipment_lib"
          classname="FadableButtonSkin">
        <layer classname="BitmapLayer" libraryID="equipment_inventoryfilter_close_button_normal">normal</layer>
        <layer classname="BitmapLayer" libraryID="equipment_inventoryfilter_close_button_rollover">over</layer>
        <layer classname="BitmapLayer" libraryID="equipment_inventoryfilter_close_button_click">clicked</layer>
    </skin>
    <skin name="equipment_combobox_filter" library="module_equipment_lib" classname="FadableButtonSkin">
        <layer classname="BitmapLayer" scale9grid="4,3,17,16" libraryID="equipment_combobox_filter_normal">normal
        </layer>
        <layer classname="BitmapLayer" scale9grid="4,3,17,16" libraryID="equipment_combobox_filter_hover">over</layer>
        <layer classname="BitmapLayer" scale9grid="4,3,17,16" libraryID="equipment_combobox_filter_active">clicked
        </layer>
    </skin>


    <!-- TEMPLATE HERE -->
    <template name="BATTLEPASS_TABBAR">
        <container name="tabbarContainer" height="20" width="260" marginTop="0" marginLeft="0"
                   marginRight="0" alignX="right" pixelSnapping="true">
            <itemlist name="tabsItemlist" registeredSkinName="commonui_list_mask" layout="horizontal" alignX="right"
                      width="202" itemGap="0" height="20" mouseEnabled="true" mouseChildren="true"
                      cacheSkinMaskAsBitmap="false">
                <itemrenderer height="15" width="101" autoSizeWidth="false" mouseEnabled="true" mouseChildren="true">
                    <button name="tab" registeredSkinName="commonui_tabbarbutton" mouseEnabled="true"
                            autoSizeWidth="true" autoSizeHeight="true" mouseChildren="true"/>
                    <button name="tab_selected" registeredSkinName="commonui_tabbarbutton" mouseEnabled="true"
                            autoSizeWidth="true" visible="false" autoSizeHeight="true" mouseChildren="true"/>
                    <label y="-1" name="tabLabel" registeredSkinName="commonui_default_label"
                           registeredTextStyleName="commonui_tabbar_center" mouseEnabled="false"
                           mouseChildren="false" autoSizeWidth="true" text="?"/>
                </itemrenderer>
            </itemlist>
        </container>
    </template>

    <template name="BATTLEPASS_OVERVIEW">
        <container name="objective_parent" width="411" marginLeft="20" marginRight="20" height="500"
                   alignX="left">
            <container name="top" width="411" marginTop="26" alignX="center">
                <label text="localize:window_objectives_completed"
                       registeredSkinName="commonui_default_label" autoSizeTextHeight="true"
                       autoSizeWidth="true" marginLeft="9" registeredTextStyleName="battlepass_rewards_headline"/>
                <label name="overall_progress" text="99/99"
                       registeredSkinName="commonui_default_label" marginLeft="253" autoSizeTextHeight="true"
                       autoSizeWidth="true" registeredTextStyleName="battlepass_rewards_headline"/>
                <coreponent width="38" height="38" marginTop="-10" marginLeft="311" registeredSkinName="battlepass_reward_icon"/>
                <coreponent width="38" height="38" marginTop="-10" marginLeft="361" registeredSkinName="battlepass_gold_reward_icon"/>
            </container>

            <container name="body" width="411" marginTop="62" height="241" marginLeft="0">
                <itemlist name="battlepass_objectiveslist" width="410"
                          autoSizeHeight="true"
                          autoSizeWidth="true"
                          itemGap="0"
                          mouseChildren="true"
                          mouseEnabled="true"
                          layout="vertical"
                          cacheSkinMaskAsBitmap="false"
                          registeredSkinName="commonui_list_mask">


                    <itemrenderer width="410" autoSizeWidth="false" height="80" mouseEnabled="true"
                                  mouseChildren="true">
                        <coreponent name="bg" width="410" autoSizeHeight="true" autoHeightMode="parent" registeredSkinName="commonui_darklayer"/>
                        <container name="obj_col" width="303" marginLeft="0" autoSizeHeight="true" autoHeightMode="parent" alignX="left">
                            <label name="title" text="test" registeredSkinName="commonui_default_label" marginTop="7" marginLeft="8"
                                   alignX="left" width="250" autoSizeTextHeight="true" registeredTextStyleName="battlepass_objectives_title"/>
                            <label name="conditions" text="test" registeredSkinName="commonui_default_label" marginTop="26" marginLeft="18"
                                   alignX="left" width="200" autoSizeTextHeight="true" registeredTextStyleName="battlepass_objectives_condition"/>
                            <label name="objective_progress" text="test" registeredSkinName="commonui_default_label" marginTop="26" marginRight="17"
                                   alignX="right" width="75" autoSizeTextHeight="true" registeredTextStyleName="battlepass_objectives_progress"/>
                            <coreponent name="obj_completed_blocker" width="306" autoSizeHeight="true" autoHeightMode="parent" registeredSkinName="battlepass_obj_completed_blocker"/>

                        </container>

                        <coreponent name="vertical_divider_1" width="1" marginLeft="303" height="80"/>

                        <container name="reward_col_1" width="52" marginLeft="303" height="80" alignX="left">
                            <coreponent name="reward_img" width="30" height="30" alignY="center" alignX="center"/>
                            <coreponent name="obj_completed_blocker" width="52" autoSizeHeight="true" autoHeightMode="parent" registeredSkinName="battlepass_obj_completed_blocker"/>
                        </container>

                        <coreponent name="vertical_divider_2" width="1" marginLeft="354" height="80"/>

                        <container name="reward_col_2" width="51" marginLeft="355" height="80" alignX="left">
                            <coreponent name="highlight_img" width="51" autoSizeHeight="true" autoHeightMode="parent" alignY="center" alignX="center" registeredSkinName="battlepass_gold_reward_blocker"/>
                            <coreponent name="reward_img" width="30" height="30" alignY="center" alignX="center"/>
                            <coreponent name="lock_img" width="18" height="23" alignY="center" alignX="center" registeredSkinName="battlepass_gold_reward_lock"/>
                            <coreponent name="obj_completed_blocker" width="51" autoSizeHeight="true" autoHeightMode="parent" registeredSkinName="battlepass_obj_completed_blocker"/>

                        </container>
                        <coreponent registeredSkinName="battlebass_outline" width="410" height="80"/>

                    </itemrenderer>


                </itemlist>
                <scrollbar name="battlepass_objectiveslist_scrollBar" width="5" autoSizeHeight="true" direction="vertical"
                           marginTop="1"
                           marginBottom="1"
                           marginRight="1"
                           alignX="right"
                           registeredSkinName="battlepass_scrollbar_bgnd">
                    <button name="buttonSlider" alignX="center" width="5" height="10"
                            registeredSkinName="commonui_scrollbar_slider"/>
                </scrollbar>
                <bind source="battlepass_objectiveslist_scrollBar" target="battlepass_objectiveslist"/>
            </container>

            <container name="bottom" width="410" marginTop="323" height="123" marginLeft="0">
                <coreponent width="123" height="123" marginLeft="0" registeredSkinName="battlepass_video_bg" />
                <videoplayer marginLeft="0" name="vid" width="120" height="120" alignX="left" alignY="center"/>
                <coreponent width="278" height="123" marginLeft="133" registeredSkinName="battlepass_description_bg" />

                <label name="description_title" width="260" autoSizeTextHeight="true" text="BAttle Pass" marginLeft="141" marginTop="5" registeredTextStyleName="battlepass_description_title"/>

                <container marginTop="26" name="description_body" alignX="left" mouseEnabled="false" height="61" width="260" marginLeft="141">
                    <scrollcontainer mouseEnabled="true" alignY="center" name="scrollContainerText" height="61" autoSizeHeight="false" width="250" registeredSkinName="battlepass_invisible">
                        <label name="content" width="250" useHTML="true" mouseEnabled="true" registeredTextStyleName="battlepass_description" text="Default" autoSizeTextHeight="true"/>
                    </scrollcontainer>

                    <scrollbar name="scrollbarText" width="6" autoSizeHeight="true" alignX="right" marginRight="1" marginBottom="1" registeredSkinName="commonui_scrollbar_bgnd_dark">
                        <button name="buttonSlider" width="4" height="61" alignX="center" registeredSkinName="commonui_scrollbar_slider"/>
                    </scrollbar>

                    <bind source="scrollbarText" target="scrollContainerText" type="scrollcontainer"/>
                </container>

                <button name="purchase_btn" localizedText="window_objectives_bp_buynow" width="156" alignY="bottom" marginBottom="5" marginLeft="200"
                        alignX="left" height="27" registeredSkinName="commonui_scale9_button_green" textMarginY="1"/>
                <button name="collect_btn" localizedText="window_objectives_bp_collect" width="156" alignY="bottom" marginBottom="5" marginLeft="200"
                        alignX="left" height="27" registeredSkinName="commonui_scale9_button_green" textMarginY="1"/>
            </container>
        </container>
    </template>

    <template name="BATTLEPASS_KEY_EXCHANGE">
        <container name="key_exchange_parent" width="411" marginLeft="20" marginRight="20" height="500"
                   alignX="left">
            <container name="top" width="411" marginTop="26" alignX="center">
                <container name="filterComboboxContainer" width="140" height="21" marginLeft="-20" mouseEnabled="false">

                    <ComboBox name="combobox.filters"
                              width="98"
                              height="17"
                              alignX="right"
                              alignY="bottom"
                              marginRight="20"
                              marginBottom="1"
                              registeredSkinName="crafting_combobox_filter"
                              popupTemplate="battlepass_template_inventory_filter_popup"/>

                    <label name="label.combobox.filters"
                           alignY="bottom"
                           alignX="right"
                           width="98"
                           height="17"
                           marginBottom="2"
                           marginRight="18"
                           registeredTextStyleName="assembly_label_filter"
                           text="localize:assembly_label_filter"/>

                    <container name="arrowContainer"
                               alignY="bottom"
                               alignX="right"
                               width="10"
                               height="10"
                               marginBottom="0"
                               marginRight="26"
                               rotation="180">

                        <container name="arrowRotationContainer"
                                   rotation="180">

                            <bitmap x="-5" y="-5" library="module_equipment_lib" libraryID="equipment_arrow_down_active"/>
                            <bitmap x="-5" y="-5" library="module_equipment_lib" libraryID="equipment_arrow_down_hover"/>
                            <bitmap x="-5" y="-5" library="module_equipment_lib" libraryID="equipment_arrow_down_normal"/>

                        </container>
                    </container>
                </container>
                <label localizedText="window_objectives_keyexchange_keys"
                       registeredSkinName="commonui_default_label" autoSizeTextHeight="true" marginLeft="170"
                       autoSizeWidth="true" registeredTextStyleName="battlepass_rewards_headline"/>
                <label name="overall_keys" text="9999"
                       registeredSkinName="commonui_default_label" marginLeft="295" autoSizeTextHeight="true"
                       autoSizeWidth="true" registeredTextStyleName="battlepass_rewards_headline"/>
                <coreponent width="35" height="35" marginTop="-10" marginLeft="340" registeredSkinName="exchange_key_token"/>
                <button name="info_btn" tooltipData="localize:window_objectives_keyexchange_keys_ttip"
                        marginLeft="380" marginTop="0" alignX="left" registeredSkinName="commonui_info_button"/>
            </container>

            <container name="body" width="411" marginTop="52" height="401" marginLeft="0">
                <itemlist name="battlepass_exchangelist" width="410"
                          autoSizeHeight="true"
                          autoSizeWidth="true"
                          itemGap="0"
                          mouseChildren="true"
                          mouseEnabled="true"
                          layout="vertical"
                          cacheSkinMaskAsBitmap="false"
                          registeredSkinName="commonui_list_mask">


                    <itemrenderer width="410" autoSizeWidth="false" height="80" mouseEnabled="true"
                                  mouseChildren="true">
                        <coreponent name="bg" width="410" autoSizeHeight="true" autoHeightMode="parent" registeredSkinName="commonui_darklayer"/>
                        <coreponent name="special_bg" width="410" autoSizeHeight="true" autoHeightMode="parent" registeredSkinName="commonui_yellowlayer"/>

                        <container name="reward" width="80" marginLeft="0" height="80" alignX="left">
                            <coreponent name="bg" width="70" height="70" alignY="center" alignX="center" registeredSkinName="exchange_reward_standard_bg"/>
                            <coreponent name="bg_special" width="70" height="70" alignY="center" alignX="center" registeredSkinName="exchange_reward_special_bg"/>
                            <coreponent name="reward_img" width="30" height="30" alignY="center" alignX="center"/>
                        </container>

                        <coreponent name="vertical_divider_1" width="1" marginLeft="81" height="80"/>

                        <container name="description" width="230" marginLeft="81" autoSizeHeight="true" autoHeightMode="parent" alignX="left">
                            <coreponent name="bg" width="220" height="70" alignY="center" alignX="center" registeredSkinName="exchange_description_standard_bg"/>
                            <coreponent name="bg_special" width="220" height="70" alignY="center" alignX="center" registeredSkinName="exchange_description_special_bg"/>
                            <label name="title" text="test" registeredSkinName="commonui_default_label" marginTop="5" marginLeft="10"
                                   alignX="left" width="220" autoSizeTextHeight="true" registeredTextStyleName="battlepass_objectives_title"/>
                            <label name="body" text="test" registeredSkinName="commonui_default_label" marginTop="30" marginLeft="10" height = "50"
                                   alignX="left" width="215" autoSizeTextHeight="true" registeredTextStyleName="battlepass_exchange_reward_description"/>
                            <label name="limit" text="test" registeredSkinName="commonui_default_label" marginBottom="10" marginLeft="10"
                                   alignX="left" alignY="bottom" width="200" autoSizeTextHeight="true" registeredTextStyleName="battlepass_exchange_reward_description"/>
                        </container>

                        <coreponent name="vertical_divider_2" width="1" marginLeft="311" height="80"/>

                        <container name="claim" width="100" marginLeft="311" height="80" alignX="left">
                            <coreponent name="key_img" marginLeft="15" marginTop="10" width="30" height="30" registeredSkinName="exchange_key_token"/>
                            <label name="require_keys" text="9999"  registeredSkinName="commonui_default_label" marginLeft="45" marginTop="17" autoSizeTextHeight="true"
                                   autoSizeWidth="true" registeredTextStyleName="battlepass_rewards_headline"/>
                            <button name="claim_btn" localizedText="window_objectives_keyexchange_exchange" width="90" alignY="bottom" marginBottom="10" marginLeft="5"
                                    alignX="left" height="25" registeredSkinName="commonui_scale9_button_blue" />
                        </container>
                        <coreponent registeredSkinName="battlebass_outline" width="410" height="80"/>

                        <coreponent name="blocker" width="408" height="78" marginLeft="1" marginTop="1" registeredSkinName="battlepass_obj_completed_blocker"/>

                    </itemrenderer>

                </itemlist>
                <scrollbar name="battlepass_exchangelist_scrollBar" width="5" autoSizeHeight="true" direction="vertical"
                           marginTop="1"
                           marginBottom="1"
                           marginRight="1"
                           alignX="right"
                           registeredSkinName="battlepass_scrollbar_bgnd">
                    <button name="buttonSlider" alignX="center" width="5" height="10"
                            registeredSkinName="commonui_scrollbar_slider"/>
                </scrollbar>
                <bind source="battlepass_exchangelist_scrollBar" target="battlepass_exchangelist"/>
            </container>

        </container>
    </template>


    <template name="battlepass_template_inventory_filter_popup">

        <root registeredSkinName="crafting_background_inventory_filter_popup"
              height="134">

            <container name="inventory.filter"
                       y="30"
                       layout="vertical"
                       gap="5"
                       autoSizeWidth="true"
                       autoWidthMode="children"
                       autoHeightMode="children"
                       mouseEnabled="true"/>

            <button name="inventory.closeButton"
                    alignX="right"
                    x="1"
                    y="5"
                    registeredSkinName="equipment_button_close_inventory_filter_popup"/>

            <checkbox name="inventory.selectAllCheckbox"
                      alignX="left"
                      marginLeft="3"
                      marginTop="9"
                      width="122"
                      height="17"
                      registeredSkinName="equipment_combobox_filter">

            </checkbox>

            <bitmap name="led.off" library="module_equipment_lib" libraryID="equipment_led_off" x="5" y="9"/>
            <bitmap name="led.on" library="module_equipment_lib" libraryID="equipment_led_on" x="5" y="9" alpha="0"/>

            <label name="inventory.selectAllLabel"
                   x="21"
                   y="9"
                   registeredTextStyleName="equipment_combobox_inventory_filter"
                   text="localize:equipment_inventory_popup.filtering_label_showall:eic"
                   autoSizeTextWidth="true"
            />
        </root>
